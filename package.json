{
  "name": "open-market-data",
  "version": "0.1.0",
  "description": "Unified CLI for free financial data APIs â€” SEC EDGAR, Yahoo Finance, Binance, CoinGecko, FRED",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "omd": "dist/cli.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/",
    "lint:fix": "biome check --write src/",
    "format": "biome format --write src/",
    "check": "biome check src/ && tsc --noEmit && vitest run"
  },
  "keywords": [
    "finance",
    "stock",
    "crypto",
    "sec-edgar",
    "fred",
    "yahoo-finance",
    "binance",
    "coingecko",
    "cli",
    "market-data"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "yahoo-finance2": "^3.13.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^25.2.3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
